# Gitify configuration for building Romanesco when patterns are managed by GPM

# This data should only be built by Gitify inside a project, never extracted!
# Don't use build --force, as this will erase all your project data!
data_directory: ../../../../_romanesco/

data:
    # Contexts
    contexts:
        class: MODX\Revolution\modContext
        primary: key

        # Only build the default ones, otherwise custom contexts will be deleted
        where:
            - 'key:IN':
                - 'web'
                - 'mgr'
                - 'global'
                - 'hub'
    context_settings:
        class: MODX\Revolution\modContextSetting
        primary:
            - context_key
            - key
        exclude_keys:
            - editedon

        # Only include settings for Hub and Global contexts
        where:
            - 'context_key:IN':
                - 'hub'
                - 'global'
            - 'key:NOT IN':
                - 'site_url'
                - 'base_url'
                - 'http_host'
                - 'site_start'

    # Basic elements
    categories:
        class: MODX\Revolution\modCategory
        primary:
            - id
            - category
        truncate_on_force:
            - modCategoryClosure

        # Exclude everything above the 100K autoincrement threshold
        where:
            - 'id:<': '100000'
    lexicon_entries:
        class: MODX\Revolution\modLexiconEntry
        primary:
            - id
            - name
        exclude_keys:
            - editedon
        where:
            - 'id:<': '100000'
    menus:
        class: MODX\Revolution\modMenu
        primary: text
        where:
            - 'namespace:IN':
                - 'clientconfig'
    property_set:
        class: MODX\Revolution\modPropertySet
        primary: name
        where:
            - 'id:<': '100000'

    # User config
    access_context:
        class: MODX\Revolution\modAccessContext
        primary: id
        where:
            - 'id:<': '100000'
    access_category:
        class: MODX\Revolution\modAccessCategory
        primary: id
        where:
            - 'id:<': '100000'
    access_media_source:
        class: MODX\Revolution\Sources\modAccessMediaSource
        primary: id
        where:
            - 'id:<': '100000'
    access_policies:
        class: MODX\Revolution\modAccessPolicy
        primary: name
        where:
            - 'id:<': '100000'
    access_resource_groups:
        class: MODX\Revolution\modAccessResourceGroup
        primary: id
        where:
            - 'id:<': '100000'
    dashboards:
        class: MODX\Revolution\modDashboard
        primary:
            - id
            - name
        where:
            - 'id:<': '100000'
    dashboard_widgets:
        class: MODX\Revolution\modDashboardWidget
        primary: id
        where:
            - 'id:<': '100000'
    dashboard_widget_placement:
        class: MODX\Revolution\modDashboardWidgetPlacement
        primary:
            - dashboard
            - widget
        where:
            - 'dashboard:<': '100000'
    #document_groups:
    #    class: MODX\Revolution\modResourceGroupResource
    #    primary: id
    #documentgroup_names:
    #    class: MODX\Revolution\modResourceGroup
    #    primary: name
    fc_profiles:
        class: MODX\Revolution\modFormCustomizationProfile
        primary: name
        where:
            - 'id:<': '100000'
    fc_profiles_usergroups:
        class: MODX\Revolution\modFormCustomizationProfileUserGroup
        primary:
